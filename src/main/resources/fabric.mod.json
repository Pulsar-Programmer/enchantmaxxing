{
	"schemaVersion": 1,
	"id": "enchantify",
	"version": "${version}",
	"name": "Final Tinted Touch",
	"description": "Mod that helps max out enchants on items.",
	"authors": [
		"Nosam08",
		"Subraid"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/Pulsar-Programmer/enchantmaxxing"
	},
	"license": "CC0-1.0",
	"icon": "assets/enchantmaxxing/icon.png",
	"environment": "client",
	"entrypoints": {
		"main": [
			"net.nosam08.enchantmaxxing.Enchantmaxxing"
		],
		"fabric-datagen": [
			"net.nosam08.enchantmaxxing.EnchantmaxxingDataGenerator"
		],
		"client": [
			"net.nosam08.enchantmaxxing.EnchantmaxxingClient"
		],


		"modmenu": [
			"net.nosam08.enchantmaxxing.config.EnchantifyModMenu"
		]
	},
	"depends": {
		"fabricloader": ">=${loader_version}",
		"minecraft": "${minecraft_version}",
		"java": ">=21",

		"fabric-api": "${fabric_version}",
		"cloth-config": "${cloth_config}",
		"owo": "${owo_version}"
	},
	"suggests": {
		"modmenu": "${mod_menu}"
	}
}